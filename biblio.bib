@inproceedings{fredouille.CAP00,
  title = {Exp\'eriences sur l'inf\'erence de langage par sp\'ecialisation},
  author = {Daniel Fredouille},
  pubtype = {conf\'erences nationales},
  booktitle = {CAp 2000},
  edition = {hermes},
  key = {CAP00_DF},
  month = {juin},
  pages = {117-130},
  year = {2000},
  fabstract = {L'apprentissage des langages r\'eguliers à partir d'exemples et de contre-exemples est un problème NP-complet [Angluin78,Gold78]. 
  Il existe des m\'ethodes qui s'attaquent à ce problème en inf\'erant des langages sous forme d'automates finis. Dans ce contexte des heuristiques performantes existent lorsque le nombre d'exemples est important [Oncina92,Lang92,Pr98]. 
  Les algorithmes d\'evelopp\'es partent habituellement d'un automate apprenant par coeur les exemples, puis le g\'en\'eralisent sous le contrôle des contre-exemples. 
L'approche propos\'ee dans cet article est compl\'ementaire: nous partons de l'automate 
le plus g\'en\'eral que nous sp\'ecialisons sous le contrôle des exemples et des contre-exemples.
},
  eabstract = {Learning a regular language from examples and counter-examples is known to be a NP-complete problem [Angluin78,Gold78]. Still, several heuristic methods have been proposed to tackle it by inferring finite automata. They give good results when the learning sample is large enough  [Oncina92,Lang92,Pr98]. These algorithms generally start from the most specialized automaton built on the examples and generalise it with respect to the counter examples. 
In this paper, we propose a different method: to start from the most general automaton and to specialize it with respect to the examples and the counter-examples. 
},
  uri = {
   dfredoui/cap2000.ps
  },
}




